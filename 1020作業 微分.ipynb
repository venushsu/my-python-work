   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "from sympy import*\n",
    "x = Symbol('x')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "輸入你想微分的x多項式(eg:x**6+2*x+7): x**6+2*x+7\n",
      "輸入你想微分的次數: 3\n"
     ]
    }
   ],
   "source": [
    "poly = input(\"輸入你想微分的x多項式(eg:x**6+2*x+7): \")\n",
    "times = input(\"輸入你想微分的次數: \")\n",
    "poly = sympify(poly)\n",
    "times = int(times)\n",
    "ans = diff(poly,x,times)"
   ]
  },
